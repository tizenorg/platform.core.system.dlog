<manifest>
	<define>
		<domain name="dlogutil"/>
	</define>
	<assign>
		<filesystem path="/usr/bin/dlog_logger" label="_" exec_label="none"/>
		<filesystem path="/usr/bin/dlogutil" label="_" exec_label="none"/>
